{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++-11 build active file",
      "command": "/usr/local/bin/g++-11",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/main.cpp",
				"${workspaceFolder}/opdracht1.cpp",
				"${workspaceFolder}/opdracht2.cpp",
				"${workspaceFolder}/opdracht3.cpp",
				"${workspaceFolder}/opdracht4.cpp",
				"${workspaceFolder}/opdracht5.cpp",
				"${workspaceFolder}/Kat.cpp",
				"${workspaceFolder}/Siamees.cpp",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}